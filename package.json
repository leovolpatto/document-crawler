   "description": "Crawler",
   "name": "craylwe",
   "displayName": "crawler",
   "license": "ISC",
   "scripts": {
      "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
      "build": "tsc",
      "test": "jest",
      "debug": "tsc && node --inspect=5858 -r ts-node/register ./src/index.ts",
      "start": "ts-node src/index.ts",
      "start:watch": "nodemon"
   },
   "dependencies": {      
      "axios": "^0.19.2",
      "dotenv": "^8.2.0",
      "express": "^4.17.3",
      "http": "^0.0.0",
      "nodemon": "^2.0.3",
      "reflect-metadata": "^0.1.10",
      "tsconfig-paths": "^3.9.0",
      "uuid": "^7.0.1"
   },
   "devDependencies": {
      "@types/express": "^4.17.2",
      "@types/node": "^8.0.29",
      "@types/reflect-metadata": "^0.1.0",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
